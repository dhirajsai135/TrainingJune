@model IEnumerable<ExcelOperation.Models.ExcelExportViewModel>



<br/>
<div class="container">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Gender</th>
                <th>Age</th>
            </tr>
        </thead>
        <tbody>
            @if(Model!=null)
            {
                foreach(var item in Model)
                {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Gender</td>
                <td>@item.Age</td>
            </tr>
                    
                }
            }
        </tbody>
    </table>
</div>
<br/>
<a href="@Url.Action("ExportToExcel","Home")" class="btn btn-success">Export to Excel</a>

